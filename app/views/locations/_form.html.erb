<%= simple_form_for(@location) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :nome %>
    <%= f.input :endereco %>
    <%= f.input :titulo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
